{
  "name": "Java SDK",
  "version": {
    "major": 0,
    "minor": 0,
    "point": 0,
    "prerelease": ""
  },
  "envVars": {
    "SDK_RESOURCE_DIR": "./resources/sdk/purecloudjava",
    "SDK_SCRIPTS_DIR": "./resources/sdk/purecloudjava/scripts"
  },
  "settings": {
    "sdkRepo": {
      "repo": "https://github.com/MyPureCloud/purecloud_api_sdk_java.git",
      "branch": "sdkv1"
    },
    "logLevel": "debug",
    "extensionsDestination": "${SDK_REPO}/build/src/main/java/com/mypurecloud/sdk",
    "swagger": {
      "oldSwaggerPath": "${SDK_REPO}/swagger.json",
      "newSwaggerPath": "https://api.mypurecloud.com/api/v2/docs/swagger",
      "saveNewSwaggerPath": "${SDK_REPO}/swagger.json"
    },
    "swaggerCodegen": {
      "language": "purecloudjava",
      "jarPath": "/git/swagger-codegen/modules/swagger-codegen-cli/target/swagger-codegen-cli.jar",
      "configFile": "${SDK_TEMP}/config.json",
      "extraGeneratorOptions": []
    },
    "debugConfig": false
  },
  "stageSettings": {
    "prebuild": {
      "preRunScripts": [
        {
          "type": "node",
          "path": "${SDK_SCRIPTS_DIR}/test.js",
          "args": [
          	"prebuild-prerun"
          ]
        }
      ],
      "postRunScripts": [
        {
          "type": "node",
          "path": "${SDK_SCRIPTS_DIR}/test.js",
          "args": [
          	"prebuild-postrun"
          ]
        }
      ]
    },
    "build": {
      "preRunScripts": [
        {
          "type": "node",
          "path": "${SDK_SCRIPTS_DIR}/test.js",
          "args": [
          	"build-prerun"
          ]
        }
      ],
      "compileScripts": [
        {
          "type": "shell",
          "path": "${SDK_SCRIPTS_DIR}/compile.sh",
          "args": []
        }
      ],
      "postRunScripts": [
        {
          "type": "node",
          "path": "${SDK_SCRIPTS_DIR}/test.js",
          "args": [
          	"build-postrun"
          ]
        }
      ]
    },
    "postbuild": {}
  }
}